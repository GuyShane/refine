doctype html
html
  head
    meta(name='viewport', content-width='width=device-width, initial-scale=1.0')
    title= title
    link(rel='stylesheet', href='/bootstrap/css/bootstrap.min.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
    script(src='javascripts/jquery-1.12.2.js')
    script(src='/bootstrap/js/bootstrap.js')
  body
    .navbar.navbar-inverse
      .container-fluid
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='#tabs')
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/') Refine Renovations
        #tabs.collapse.navbar-collapse
          ul.nav.navbar-nav.navbar-right
            each vals,link in navbar_links
              if vals.active
                li.active
                  a(href=vals.destination)= link
              else
                li
                  a(href=vals.destination)= link
    block content
