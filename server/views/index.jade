extends layout

block content
  .content
    .container-fluid
      img#logo(src='/images/logo.png')
      each paragraph in paragraphs
        p.about= paragraph
    .container-fluid
      h1#services_title Services
      .row
        each service in services
          .col-xs-12.col-sm-6.col-md-4
            .service
              img.serive_icon(src=service.src)
              p= service.description